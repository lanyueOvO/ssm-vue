is.$http.post('/slideshow/changedStatus',info).then((res)=>{\r\n        // console.log(res);\r\n        if(res.data.code == 200){\r\n          this.$message.success('修改成功！')\r\n        }else if(res.data.code ==400){\r\n          this.$message.error('修改失败！超过最大长度！')\r\n          info.status = false\r\n        }\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n</style>"]}]}